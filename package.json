{
  "name": "feishu-mcp-server",
  "version": "0.0.1",
  "description": "FeiShu MCP Server",
  "scripts": {
    "check": "biome check --write",
    "format": "biome format --write",
    "build": "tsup",
    "start": "nodemon",
    "inspect": "pnpx @modelcontextprotocol/inspector",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=23.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdd330/feishu-mcp-server.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "preferGlobal": true,
  "bin": {
    "feishu-mcp-server": "dist/index.js"
  },
  "keywords": ["feishu", "mcp"],
  "author": "yang.leijun@gmail.com",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@modelcontextprotocol/sdk": "1.7.0",
    "axios": "1.8.4",
    "dotenv": "16.4.7",
    "fastify": "5.2.2",
    "qs": "6.13.0",
    "tslib": "2.8.1",
    "winston": "3.17.0",
    "yargs": "17.7.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.11",
    "@types/qs": "6.9.16",
    "@types/yargs": "17.0.33",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "15.5.0",
    "nodemon": "3.1.9",
    "simple-git-hooks": "^2.12.1",
    "ts-jest": "^29.3.2",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "zx": "8.4.1"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": ["biome format --write", "biome check --apply"]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "pnpm build"
  }
}
